@model IEnumerable<Web_Application_Higher_Institution.table_campus_add>

@{
    ViewBag.Title = "Index";
}
<h1>Admin Module</h1>

<h3 class="text-center text-info">Current List of Campus</h3>
<hr />
<p>
    @Html.ActionLink("Add New Campus", "Create")
</p>
<table class="table table-bordered table-condensed table-hover table-responsive table-striped text-center">
    <tr>
        <th class="text-center">
            @Html.DisplayNameFor(model => model.campus)
        </th>
        <th class="text-center">
            Last Updated Date
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.campus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dat)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.p_id }) |
                @Html.ActionLink("Details", "Details", new { id = item.p_id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.p_id })
            </td>
        </tr>
    }

</table>
