@model Web_Application_Higher_Institution.table_calendar_year

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<h2>Dev. Module</h2>

<h4 class="text-center text-info">Add Academic Year</h4>
<hr />
<h5 class="text-center text-danger">@ViewBag.Result</h5>
@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal col-lg-6" style=" float: none; margin: 0 auto;">

        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
           <label class = "control-label col-md-2">Year</label>
             <div class="col-md-10">
                @Html.EditorFor(model => model.calendar_year, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.calendar_year, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Add Academic Year" class="btn btn-primary" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
